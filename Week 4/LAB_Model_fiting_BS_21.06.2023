   "source": [
    "### Using Ordinal Encoder for Ordinal Columns"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 132,
   "id": "afd7fa93",
   "metadata": {},
   "outputs": [],
   "source": [
    "from sklearn.preprocessing import OrdinalEncoder\n",
    "\n",
    "education_hierarchy = ['high school or below', 'college', 'bachelor', 'master', 'doctor']\n",
    "coverage_hierarchy = ['basic', 'extended', 'premium']\n",
    "policy_hierarchy = ['personal l1', 'personal l2', 'personal l3', 'corporate l1', 'corporate l2',\n",
    "                     'corporate l3', 'special l1', 'special l2', 'special l3']\n",
    "renew_offer_type_hierarchy = ['offer1', 'offer2', 'offer3', 'offer4']\n",
    "vehicle_size_hierarchy = ['small', 'medsize', 'large']\n",
    "month_hierarchy = ['jan','feb']\n",
    "location_code_hierarchy = ['rural','suburban','urban']\n",
    "sales_channel_hierarchy = ['agent', 'call center', 'branch', 'web']\n",
    "vehicle_class_hierarchy = ['two-door car', 'four-door car', 'sports car', 'suv', 'luxury car', 'luxury suv']\n",
    "\n",
    "encoder1 = OrdinalEncoder(categories=[education_hierarchy])\n",
    "encoder2 = OrdinalEncoder(categories=[coverage_hierarchy])\n",
    "encoder3 = OrdinalEncoder(categories=[policy_hierarchy])\n",
    "encoder4 = OrdinalEncoder(categories=[renew_offer_type_hierarchy])\n",
    "encoder5 = OrdinalEncoder(categories=[vehicle_size_hierarchy])\n",
    "encoder6 = OrdinalEncoder(categories=[month_hierarchy])\n",
    "encoder7 = OrdinalEncoder(categories=[location_code_hierarchy])\n",
    "encoder8 = OrdinalEncoder(categories=[sales_channel_hierarchy])\n",
    "encoder9 = OrdinalEncoder(categories=[vehicle_class_hierarchy])\n",
    "\n",
    "ordinal_encoded = pd.DataFrame({\n",
    "    'education_encoded': encoder1.fit_transform(df[['education']]).flatten().astype(int),\n",
    "    'coverage_encoded': encoder2.fit_transform(df[['coverage']]).flatten().astype(int),\n",
    "    'policy_encoded': encoder3.fit_transform(df[['policy']]).flatten().astype(int),\n",
    "    'renew_offer_type_encoded': encoder4.fit_transform(df[['renew_offer_type']]).flatten().astype(int),\n",
    "    'vehicle_size_encoded': encoder5.fit_transform(df[['vehicle_size']]).flatten().astype(int),\n",
    "    'month_encoded': encoder6.fit_transform(df[['month']]).flatten().astype(int),\n",
    "    'location_code_encoded': encoder7.fit_transform(df[['location_code']]).flatten().astype(int),\n",
    "    'sales_channel_encoded': encoder8.fit_transform(df[['sales_channel']]).flatten().astype(int),\n",
    "    'vehicle_class_encoded': encoder9.fit_transform(df[['vehicle_class']]).flatten().astype(int)\n",
    "})"
   ]
  },
